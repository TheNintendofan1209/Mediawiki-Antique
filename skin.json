{
	"name": "Antique",
	"version": "1.0",
	"author": "...",
	"url": "https://mike9012.xyz",
	"descriptionmsg": "antique-desc",
	"namemsg": "skinname-antique",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"ValidSkinNames": {
		"antique": "Antique"
	},
	"MessagesDirs": {
